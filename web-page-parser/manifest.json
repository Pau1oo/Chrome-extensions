{
  "name": "Realt.by parser",
  "description": "This extension parses ads on realt.by and sends the data to a Google Sheets table.",
  "version": "1.0.3",
  "manifest_version": 3,
  "permissions": [
    "contextMenus",
    "activeTab",
    "identity",
    "storage",
    "scripting",
    "tabs",
    "notifications",
    "alarms"
  ],
  "host_permissions": [
    "*://*.realt.by/*",
    "https://raw.githubusercontent.com/*"
  ],
  "oauth2": {
    "client_id": "101166783378-o9rpg0qqmllh4tr01g21380tfd9avpt9.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/spreadsheets"
    ]
  },
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "images/parser-16.png",
    "24": "images/parser-24.png",
    "32": "images/parser-32.png",
    "128": "images/parser-128.png"
  },
  "action": {
    "default_icon": {
      "16": "images/parser-16.png",
      "24": "images/parser-24.png",
      "32": "images/parser-32.png",
      "128": "images/parser-128.png"
    },
    "default_popup": "popup/popup.html"
  },
  "content_scripts": [ ]
}